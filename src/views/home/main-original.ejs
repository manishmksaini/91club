	<style>
		.top_game {
			display: grid;
			grid-template-columns: 1fr 1fr 1fr;
			gap: 10px;
		}

		.top_list {
			display: flex;
			flex-direction: column;
			align-items: center;
			gap: 5px;
		}

		.splash_root {
			width: 100vw;
			height: 100vh;
			top: 0px;
			bottom: 0px;
			left: 0px;
			right: 0px;
			position: fixed;
			z-index: 100000;
			display: flex;
			align-items: center;
			justify-content: center;
		}

		.splash_screen {
			width: 100vw;
			height: 100vh;
			background: #07111e;
			background-image: radial-gradient(ellipse at top, #07111e, #07111e);
			max-width: 420px;
			display: flex;
			align-items: center;
			justify-content: center;
			flex-direction: column;
		}

		.splash__text {
			font-size: 22px;
			text-align: center;
			font-weight: 700;
			color: #d9b38c;
		}

		.main_box {
			width: 80%;
			flex-direction: column;
			display: flex;
			align-items: center;
			justify-content: center;
			gap: 30px;
		}

		.splash_img {
			width: 100%;
		}

		.logo_box {
			margin-top: 150px;
		}

		.logo__image {
			max-width: 200px;
			height: auto;
		}

		.game-container {
			display: flex;
			justify-content: center;
			padding: 5px;
		}

		.game-item {
			text-align: center;
			box-sizing: border-box;
		}

		.game-link {
			display: block;
			text-decoration: none;
			color: #000;
		}

		.game-image {
			width: 118px;
			border-radius: 10px;
			height: 118px;
			object-fit: cover;
		}

		.download-button {
			position: fixed;
			bottom: 2.4rem;
			display: flex;
			align-items: center;
			justify-content: space-between;
			width: 220px;
			height: 45px;
			padding: 0 15px;
			background: linear-gradient(180deg, #2ad55d 0%, #2ad55d 100%);
			color: #ffffff;
			font-size: bold;
			border-radius: 30px;
			box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
			transition: opacity 0.3s ease, visibility 0.3s ease;
			font-size: 13px;
			font-weight: 700;
			z-index: 100;
		}

		.download-button.hidden {
			opacity: 0;
			visibility: hidden;
			pointer-events: none;
		}

		Styles for overlay and popup .overlay {
			position: fixed;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
			background-color: rgba(0, 0, 0, 0.75);
			Semi-transparent black overlay display: flex;
			justify-content: center;
			align-items: center;
			z-index: 999;
			opacity: 1;
			transition: opacity 0.5s ease;
		}


		.fade-in {
			opacity: 1;
		}


		#newPopupOverlay {
			position: fixed;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
			background: rgba(0, 0, 0, 0.5);
			z-index: 2001;
			display: none;
		}

		#newPopup {
			display: none;
			position: fixed;
			height: -webkit-fill-available;

			z-index: 2002;
			background: #ffffff00;

			border-radius: 10px;
			box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
		}

		.step {
			font-size: 0.3rem;
		}


		#loading-screen {
			display: flex;
			justify-content: center;
			align-items: center;
			width: 100%;
			height: 100%;
			background-color: #ffffff;
			position: fixed;
			top: 0;
			left: 0;
			z-index: 5000;
			/* Ensure it's above everything else */
		}

		#main-content {
			display: none;
			/* Hide the main content initially */
		}

		#loading-image {

			max-height: 100vh;

		}

		@media only screen and (max-width: 600px) {
			#loading-image {
				width: 100%;
			}
		}

		.deposit-overlay {
			position: absolute;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
			background-color: rgba(0, 0, 0, 0.3);
			z-index: 1000;
		}

		.recharge-modal {
			width: 85%;
			height: 70%;
			background-color: #333332;
			/* padding: 20px; */
			border-radius: 10px;
			box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
			position: fixed;
			top: 50%;
			left: 50%;
			transform: translate(-50%, -50%);
			box-sizing: border-box;
			/* overflow: hidden; */
			display: none;
			flex-direction: column;
		}

		.recharge-modal__header {
			/* width: 100%; */
			/* text-align: center; */
			/* margin-bottom: 10px; */
			min-height: 1.86667rem;
			background-color: #4D4D4C;
			color: #F5F3F0;
			padding-top: .33333rem;
			border-radius: .26667rem .26667rem 0 0;
			text-align: center;

			& .title {
				font-size: .37333rem;
			}

			& .tip {
				font-size: .29333rem;
				font-weight: 400;
				margin-top: .26667rem;
			}
		}

		.recharge-modal__content {
			display: flex;
			justify-content: center;
			align-items: center;
			gap: 0.3rem;
			flex-wrap: wrap;
			margin: 10px 0;
			width: 100%;
			height: 100%;
			overflow: auto;
			box-sizing: border-box;

			& .container {
				padding: .26667rem .13333rem;
				background-color: #333332;
				max-height: calc(100vh - 8.4rem);
				/* overflow: auto; */

				& .item {
					background: #4D4D4C;
					padding: .26667rem;
					border-radius: .26667rem;

