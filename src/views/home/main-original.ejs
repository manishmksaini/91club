		<script>
			document.addEventListener('DOMContentLoaded', function () {
				const downloadButton = document.getElementById('downloadButton');
				const closeButton = document.getElementById('closeButton');

				if (sessionStorage.getItem('buttonHidden') === 'true') {
					downloadButton.classList.add('hidden');
				}

				closeButton.addEventListener('click', function () {
					downloadButton.classList.add('hidden');
					sessionStorage.setItem('buttonHidden', 'true');
				});
			});
			// Add click event to download button
			document.getElementById('downloadBtn').addEventListener('click', function () {
				window.location.href = './downloads/Rika_Baji.apk';
			});

			const observer = new MutationObserver((mutations) => {
				mutations.forEach((mutation) => {
					const iframe = document.querySelector('iframe[title="chat widget"]');
					if (iframe) {
						iframe.style.bottom = "83px";  // Override the 'bottom' property
						observer.disconnect();  // Stop observing after applying the style
					}
				});
			});

			observer.observe(document.body, { childList: true, subtree: true });

		</script>

