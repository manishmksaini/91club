						& .title {
							font-size: .37333rem;
							color: #F5F3F0;

							&>span {
								color: #DD9138;
								margin-left: 10px;
							}
						}

						& .orange {
							font-size: .34667rem;
							color: #DD9138;
						}
					}

					& .description {
						font-size: .29333rem;
						color: #A8A5A1;
						margin-bottom: .26667rem;
					}

					& .foot {
						display: -webkit-box;
						display: -webkit-flex;
						display: flex;
						-webkit-box-align: center;
						-webkit-align-items: center;
						align-items: center;

						&>div {
							-webkit-box-flex: 1;
							-webkit-flex: 1;
							flex: 1;
						}

						& .progress {
							width: 100%;
							position: relative;
							overflow: hidden;

							& .step {
								position: absolute;
								top: 50%;
								font-size: 12px;
								left: 50%;
								-webkit-transform: translateX(-50%) translateY(-50%);
								transform: translate(-50%) translateY(-50%);
								color: #F5F3F0;
							}
						}

						& .btn {
							width: 2.13333rem;
							min-height: .66667rem;
							display: -webkit-box;
							display: -webkit-flex;
							display: flex;
							-webkit-box-align: center;
							-webkit-align-items: center;
							align-items: center;
							-webkit-box-pack: center;
							-webkit-justify-content: center;
							justify-content: center;
							-webkit-box-flex: 0;
							-webkit-flex: none;
							flex: none;
							font-size: .32rem;
							margin-left: .56rem;
							border-radius: .13333rem;
							padding: 0 .13333rem;
							color: #8F5206;
						}

						& .btn.n2 {
							border: .01333rem solid #DD9138;
							color: #DD9138;
						}
					}
				}
			}
		}

		.recharge-modal__footer {
			/* display: flex;
        flex-direction: column;
        align-items: center;
